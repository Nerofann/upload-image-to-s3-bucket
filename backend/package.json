{
  "name": "upload-image-backend",
  "version": "1.0.0",
  "description": "Backend API for upload images to AWS S3",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "pm2:start": "pm2 start ../ecosystem.config.cjs",
    "pm2:stop": "pm2 stop upload-image-backend",
    "pm2:restart": "pm2 restart upload-image-backend",
    "pm2:delete": "pm2 delete upload-image-backend",
    "pm2:logs": "pm2 logs upload-image-backend",
    "pm2:monit": "pm2 monit"
  },
  "keywords": ["express", "aws", "s3", "upload"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "dotenv": "^16.3.1"
  }
}
